                    <xsd:element ref="OrderTypeCode" minOccurs="0" maxOccurs="1" />
                    <xsd:element ref="PartsOrderReceivedDateTime" minOccurs="0" maxOccurs="1"/>
                    <xsd:element ref="ShipmentCarrierDetail" minOccurs="0" maxOccurs="unbounded"/>                    
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
