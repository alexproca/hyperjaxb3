<hj:persistence xmlns="http://hyperjaxb3.jvnet.org/ejb/schemas/customizations" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:hj="http://hyperjaxb3.jvnet.org/ejb/schemas/customizations" xmlns:orm="http://java.sun.com/xml/ns/persistence/orm">
	<hj:default-entity class="##default">
	</hj:default-entity>
	<hj:default-id name="Hjid" javaType="java.lang.Long" schemaType="xsd:long">
		<orm:column/>
		<orm:generated-value strategy="AUTO"/>
	</hj:default-id>
	<hj:default-version>
		<orm:column/>
	</hj:default-version>
	<hj:default-basic>
		<orm:column/>
	</hj:default-basic>
	<hj:default-to-one>
		<many-to-one/>
	</hj:default-to-one>
	<hj:default-to-many>
		<one-to-many/>
	</hj:default-to-many>
	<hj:default-many-to-one>
		<orm:join-column/>
		<orm:cascade>
			<orm:cascade-all/>
		</orm:cascade>
	</hj:default-many-to-one>
	<hj:default-one-to-many>
		<orm:join-column/>
		<orm:cascade>
			<orm:cascade-all/>
		</orm:cascade>
	</hj:default-one-to-many>
	<hj:default-one-to-one>
		<orm:join-column/>
		<orm:cascade>
			<orm:cascade-all/>
		</orm:cascade>
	</hj:default-one-to-one>
	<hj:default-many-to-many>
		<orm:join-table/>
		<orm:cascade>
			<orm:cascade-all/>
		</orm:cascade>
	</hj:default-many-to-many>
</hj:persistence>
