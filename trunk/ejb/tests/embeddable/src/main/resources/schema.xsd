<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:hj="http://hyperjaxb3.jvnet.org/ejb/schemas/customizations" xmlns:orm="http://java.sun.com/xml/ns/persistence/orm"
	jaxb:extensionBindingPrefixes="hj orm" jaxb:version="2.0">
	<xs:annotation>
		<xs:appinfo>
			<jaxb:globalBindings>
				<jaxb:serializable/>
			</jaxb:globalBindings>
			<jaxb:schemaBindings>
				<jaxb:package name="org.jvnet.hyperjaxb3.ejb.tests.embeddable"/>
			</jaxb:schemaBindings>
		</xs:appinfo>
	</xs:annotation>
	<!-- ... -->
	<xs:element name="a" type="aType"/>
	<xs:complexType name="aType">
		<xs:sequence>
			<xs:element name="b" type="bType" minOccurs="0"/>
			<xs:element name="b1" type="bType" minOccurs="0"/>
			<xs:element name="e" type="eType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="bType">
		<xs:annotation>
			<xs:appinfo>
				<hj:embeddable/>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="c" type="xs:string" minOccurs="0"/>
			<xs:element name="d" type="xs:int" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="eType">
		<xs:annotation>
			<xs:appinfo>
				<hj:embeddable/>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="f" type="xs:string" minOccurs="0"/>
			<xs:element name="g" type="aType" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<hj:ignored/>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="employees">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="employee" type="EmployeeType" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="EmployeeType">
		<xs:sequence>
			<xs:element name="employeeId" type="xs:string" nillable="false"/>
			<xs:element name="firstName" type="xs:string" nillable="true" minOccurs="0"/>
			<xs:element name="lastName" type="xs:string" nillable="true" minOccurs="0"/>
			<xs:element name="addressId" type="xs:string" nillable="true" minOccurs="0"/>
			<xs:element name="primaryPhone" nillable="true" minOccurs="0" type="PrimaryPhoneType">
				<xs:annotation>
					<xs:appinfo>
						
						<hj:embedded>
							<orm:attribute-override name="phoneNumber">
								<orm:column name="phone_no"/>
							</orm:attribute-override>
							<orm:attribute-override name="phoneType">
								<orm:column name="phone_type_id"/>
							</orm:attribute-override>
						</hj:embedded>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PrimaryPhoneType">
		<xs:annotation>
			<xs:appinfo>
				<hj:embeddable/>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="phoneNumber" type="xs:string" nillable="false"/>
			<xs:element name="phoneType" type="xs:string" nillable="false"/>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="entityWithEmbeddedId" type="entityWithEmbeddedIdType"/>
	<xs:complexType name="entityWithEmbeddedIdType">
		<xs:sequence>
			<xs:element name="id" type="embeddableIdType">
				<xs:annotation>
					<xs:appinfo>
						<hj:embedded-id/>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="embeddableIdType">
		<xs:annotation>
			<xs:appinfo>
				<hj:embeddable/>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id1" type="xs:long" minOccurs="0"/>
			<xs:element name="id2" type="xs:long" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	
</xs:schema>
